end
poke(0x5f2d, 1)
function mouse()
  mouse_x = stat(32)
  mouse_y = stat(33)
  mouse_buttons = stat(34)
		if band(mouse_buttons,1)==1 then
		  lmb=true
		else
		  lmb=false
		end
		if band(mouse_buttons,2)==2 then
		  rmb=true
		else
		  rmb=false
		end
		if band(mouse_buttons,4)==4 then
		  mmb=true
		else
		  mmb=false
		end    
end
function widget(x,y,w,h,t)
 rectfill(x,y,w,h,0)
 rectfill(x+1,y+1,w-1,h-2,9)
 rect(x+1,y+h-11,w-1,y+h-11,4)
 rect(x+1,y+1,w-1,h-2,15)
 print(t,x+2,y+2,2)
end
function room(x,y,w,h,f,r) 

 rectfill(x,h+1,w,h+3,4)--shadow
 rectfill(x,y,w,h,9)--floor
 rectfill(x,y,w,y+3,4) --inside wall
 rectfill(x-2,y+3,x-1,h+3,2) --outside wall
  if r==true or r>=1 then --roof
 rectfill(x,y,w,h,15)
 else
 rect(x,y,w,h,15)
 end
end
t=0
y=0
function river()


for i=0,128 do
circfill(i,120+cos(i/75)*7,(cos(t/100)*2)+10,12)
end

t = t + 01

end

function _init()
 rooms()
 room_x()
 room_y()
 room_w()
 room_h()
-- grass()
end


