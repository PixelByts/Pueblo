py=64
ox=100
oy=10
name="mewtown"
clay=100
pop=100
startup=true
darken=false
--music(0)


function _update()
 mouse()
 if btnp(5) then
  startup=false
 end
 if btn(0) then
  darken=true
 else
  darken=false
 end
 
  if rx[current]/2+rw[current]/2>px then px+=1+rnd(.5) end
  if rx[current]/2+rw[current]/2<px then px-=1+rnd(.5) end
  if rh[current]>py then py+=1+rnd(.5) end
  if rh[current]<py then py-=1+rnd(.5) end
 
  if rx[current]/2+rw[current]/2>ox then ox+=1+rnd(.5) end
  if rx[current]/2+rw[current]/2<ox then ox-=1+rnd(.5) end
  if rh[current]>oy then oy+=1+rnd(.5) end
  if rh[current]<oy then oy-=1+rnd(.5) end

 if lmb then rx[current]=mouse_x ry[current]=mouse_y end
 if rmb then rw[current]=mouse_x rh[current]=mouse_y end
 if btnp(2) then roomoption+=1 end
 if btnp(3) then roomoption-=1 end
 if roomoption<0 then roomoption=6 end
 if roomoption>6 then roomoption=0 end
 if rx[current]>rw[current] then rx[current]=rw[current] end
 if ry[current]>rh[current] then ry[current]=rh[current] end 
 if btnp(4) then current+=1 end
 if mouse_y>95 then mouse_y=95 end
 if rh[current]>96 then rh[current]=96 end 
end

function _draw()

if startup==false then
cls(10)
river()
for i=1,maxrooms do
 room(rx[i],ry[i],rw[i],rh[i],1,0)
end
 printl(name .. "  clay " .. clay .. "  pop " .. pop,2,2,9,4,true)
 spr(0,mouse_x,mouse_y)
 print(mouse_y,1,30,0)
  people(px+rnd(1),py+rnd(1),8,"adult")
    people(ox+rnd(1),oy+rnd(1),12,"child")
 --widget(rx[1],ry[1]-11,roomoption)
 printl(rn[roomoption],rx[current],ry[current]-9,10,9,0)
 printl("",mouse_x+4,mouse_y,10,9,true)

if darken then
 map(0,0,0,0,16,16)
 widget(3,3,124,124,"hello world")
 map(16,0,10,10,16,16)
end
else
 startup()
end
end

