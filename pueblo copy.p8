function people(x,y,c,a)
 if a=="adult" then
  print(".",x,y-2,15)
  print(".",x,y-1,c)
  print(".",x,y,c)
 else
  print(".",x,y-1,15)
  print(".",x,y,c)
 end
end

function rooms()
 rn={}
 rn[0]=nil
 rn[1]="residental"
 rn[2]="kitchen"
 rn[3]="food storage"
 rn[4]="material storage"
 rn[5]="misc storage"
 rn[6]="mine"

end

function room_x()
 rx={}
 rx[1]=x
 for i=2,100 do
 rx[i]=-100
 end
end
function room_y()
 ry={}
 ry[1]=y
 for i=2,maxrooms do
 ry[i]=-100
 end
end
function room_w()
 rw={}
 rw[1]=w
 for i=2,maxrooms do
 rw[i]=-100
 end
end
function room_h()
 rh={}
 rh[1]=h
 for i=2,maxrooms do
 rh[i]=-100
 end
end



function printl(text,x,y,ic,oc,h)
 print(text,x-1,y-1,oc) 
 print(text,x,y-1,oc)
 print(text,x+1,y-1,oc)
 print(text,x-1,y,oc)
 print(text,x+1,y,oc)
 print(text,x-1,y+1,oc)
 print(text,x,y+1,oc)
 print(text,x+1,y+1,oc) 
 if h==true or h==0 then
  print(text,x-1,y+2,oc)
  print(text,x,y+2,oc)
  print(text,x+1,y+2,oc)
 end
  print(text,x,y,ic)
  --function by pixel‡
end
